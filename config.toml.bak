  # Hero Section
  [params.hero]
    title = "Poukładane"
    description = "Pracownia psychologiczna"
    buttonText = "Dowiedz się więcej"

  # intro section
  [params.intro]
    enable = true
    description = "Zmiany towarzyszą nam na różnych etapach naszego życia. Na różnych poziomach życia mamy potrzebę ładu. Ładu na biurku, przy którym na co dzień pracujemy. Ładu w szafie, torebce, samochodzie. Układamy ubrania, poduszkę przed snem, układamy sobie relacje w pracy, z bliskimi, w domu. W końcu potrzebujemy ładu w głowie, wiedzieć w którym kierunku się zmierza, by było to bardziej nasze, nieprzypadkowe. Poukładane."

  # about section
  [params.about]
    enable = true
    title = "O mnie"
    description = "Pracuję jako psycholog od 2009 roku. Mam doświadczenie pracy z osobami dotkniętymi szeroko rozumianą stratą (utratą pracy, zdrowia, żałobą po bliskiej osobie) i osobami dokonującymi zmian w swoim życiu. Ci, którzy trafiają do mnie, mogą bez obaw przed oceną mówić o swoich uczuciach, działaniach, dzielić się swoimi myślami. Wierzę, że nie ma uniwersalnych rozwiązań. Ten sam problem dla dwóch różnych osób może stanowić inne wyzwanie i w inny sposób może być przez nie tak samo skutecznie rozwiązany.<br/><br/>Wierzę, że osoba, która przychodzi po wsparcie, jest najlepszym ekspretem od swojego życia, a największe zmiany dokonują się poza gabinetem."
    buttonText = "Dalej"

  [params.about2]
    enable = true
    description = "<br/>Doświadczenie zawodowe:<br/>• Dom Hospicyjny im. św. Józefa w Sopocie<br/>• Miejski Ośrodek Pomocy Rodzinie w Gdańsku<br/>• Stowarzyszenie 'Światło i Cienie'"
    buttonText = "Dalej"
    # the picture is storead at static/images/
    picture = "header.jpg"

  # skill section
  [params.skill_list]
    enable = true
    title = "Metoda"
    description = "Terapia skoncentrowana na rozwiązaniach skupia się na potrzebach i zasobach, które posiada człowiek, aby rozwiązać nurtujące zagadnienie. To co jest celem i jaki sposób jego realizacji jest formułowany przez klienta. Osoba, która przychodzi na terapię TSR jest traktowana jako ekspert, który wie najlepiej jaka droga jest dla niego najlepsza. Zadaniem terapeuty jest prowadzenie spotkań tak, aby wspierać klienta w tworzeniu i realizacji wizji wytyczonych celów.<br/><br/>Cechą charakterystyczną terapii skoncentrowanej na rozwiązaniach jest krótkoterminowość. W praktyce oznacza to, że sesje mają mieć charakter zwięzły, a terapia ma sprowadzać się do najmniejszej możliwej liczby sesji, potrzebnych aby osiągnąć zamierzony przez klienta cel."
    buttonText = "Dalej"
    # the picture is storead at static/images/
    # picture = "pic02.jpg"

